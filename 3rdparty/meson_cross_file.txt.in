
[binaries]
c = '@MESON_CC@'
cpp = '@MESON_CPP@'
ar = '@CMAKE_AR@'
ld = '@CMAKE_LINKER@'
strip = '@CMAKE_STRIP@'
pkgconfig = '@PKG_CONFIG_EXECUTABLE@'

[properties]
needs_exe_wrapper = true

[host_machine]
system = '@MESON_SYSTEM@'
cpu_family = '@MESON_CPU_FAMILY@'
cpu = '@MESON_CPU@'
endian = '@MESON_ENDIAN@'

[paths]
prefix = '@THIRDPARTY_PREFIX@/'
libdir = 'lib/'
bindir = 'bin/'
